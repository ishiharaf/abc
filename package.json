{
	"name": "abc",
	"displayName": "ABC Tools",
	"description": "ABC Tools for VS Code",
	"version": "0.0.1",
	"license": "SEE LICENSE IN LICENSE.md",
	"categories": [
		"Other"
	],
	"keywords": [
		"abc", "notation", "music"
	],
	"engines": {
		"vscode": "^1.74.2"
	},
	"main": "src/extension.js",
	"activationEvents": [
		"onLanguage:abc"
	],
	"contributes": {
		"languages": [
			{
				"id": "abc",
				"aliases": [
					"ABC"
				],
				"extensions": [
					".abc"
				]
			}
		],
		"commands": [
			{
				"title": "ABC: Show Preview",
				"command": "abc.showPreview",
				"when": "previewEnabled"
			}
		]
	}
}