{
	"name": "abc-tools",
	"displayName": "ABC Tools",
	"description": "Preview and play ABC files in Visual Studio Code",
	"version": "0.2.1",
	"license": "SEE LICENSE IN LICENSE.md",
	"publisher": "ishiharaf",
	"author": {
		"name": "Ishihara Felipe"
	},
	"icon": "media/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ishiharaf/abc"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"abc",
		"notation",
		"music"
	],
	"engines": {
		"vscode": "^1.74.2"
	},
	"main": "src/extension.js",
	"activationEvents": [
		"onLanguage:abc"
	],
	"contributes": {
		"languages": [
			{
				"id": "abc",
				"aliases": [
					"ABC"
				],
				"extensions": [
					".abc"
				]
			}
		],
		"commands": [
			{
				"title": "ABC: Show Preview",
				"command": "abc.showPreview",
				"when": "editorLangId == abc"
			}
		]
	}
}
